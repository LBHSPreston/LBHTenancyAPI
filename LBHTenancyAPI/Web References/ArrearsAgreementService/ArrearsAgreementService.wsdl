    </wsdl:operation>
    <wsdl:operation name="GetArrearsAgreementList">
      <wsdl:documentation>&lt;summary&gt;
            This method will be used to retrieve a list of arrears agreement records. 
            It will be possible to retrieve this data by using a number of search criteria 
            &lt;/summary&gt;
            &lt;param name="dto"&gt;&lt;/param&gt;
            &lt;returns&gt;&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.civica.co.uk/UHWeb/2013/03/IArrearsAgreementService/GetArrearsAgreementList" message="tns:IArrearsAgreementService_GetArrearsAgreementList_InputMessage" />
