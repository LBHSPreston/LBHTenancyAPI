    </wsdl:operation>
    <wsdl:operation name="AddPaymentSchedule">
      <wsdl:documentation>&lt;summary&gt;
            This method provides the ability to add new repayment schedule to an existing 
            arrears agreement in UHT based on the Arrears Agreement Reference passed in. 
            &lt;/summary&gt;
            &lt;param name="dto"&gt;Details of the new repayment.&lt;/param&gt;
            &lt;returns&gt;&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.civica.co.uk/UHWeb/2013/03/IArrearsAgreementService/AddPaymentSchedule" message="tns:IArrearsAgreementService_AddPaymentSchedule_InputMessage" />
      <wsdl:output wsaw:Action="http://schemas.civica.co.uk/UHWeb/2013/03/IArrearsAgreementService/AddPaymentScheduleResponse" message="tns:IArrearsAgreementService_AddPaymentSchedule_OutputMessage" />
    </wsdl:operation>
    <wsdl:operation name="CreateArrearsAction">
      <wsdl:documentation>&lt;summary&gt;
            This method will be used to create an arrears action for a specific tenancy agreement within UHT. 
            The arrears action will appear on the Arrears Actions form.
            &lt;/summary&gt;
            &lt;param name="dto"&gt;Details of the new arrears action to add&lt;/param&gt;
            &lt;returns&gt;Success&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.civica.co.uk/UHWeb/2013/03/IArrearsAgreementService/CreateArrearsAction" message="tns:IArrearsAgreementService_CreateArrearsAction_InputMessage" />
