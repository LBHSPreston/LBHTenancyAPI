    </wsdl:operation>
    <wsdl:operation name="UpdateArrearsAgreement">
      <wsdl:documentation>&lt;summary&gt;
            This method provides the ability to update an existing arrears agreement in UHT 
            based on the Arrears Agreement Reference passed in. As part of the update process it 
            will also allow existing repayment schedule to be updated. 
            &lt;/summary&gt;
            &lt;param name="dto"&gt;&lt;/param&gt;
            &lt;returns&gt;&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.civica.co.uk/UHWeb/2013/03/IArrearsAgreementService/UpdateArrearsAgreement" message="tns:IArrearsAgreementService_UpdateArrearsAgreement_InputMessage" />
