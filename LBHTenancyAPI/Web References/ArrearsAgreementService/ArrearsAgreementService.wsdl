    <wsdl:operation name="CreateArrearsAgreement">
      <wsdl:documentation>&lt;summary&gt;
            This method provides the ability to create an arrears agreement in UHT. 
            &lt;/summary&gt;
            &lt;param name="dto"&gt;Details of the new arrears agreement&lt;/param&gt;
            &lt;returns&gt;&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.civica.co.uk/UHWeb/2013/03/IArrearsAgreementService/CreateArrearsAgreement" message="tns:IArrearsAgreementService_CreateArrearsAgreement_InputMessage" />
